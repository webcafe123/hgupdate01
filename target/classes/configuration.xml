<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias alias="board" type="com.pr.project.model.Board" />
		<typeAlias alias="mypage" type="com.pr.project.model.User" />
		<typeAlias alias="reply" type="com.pr.project.model.Reply" />
		<typeAlias alias="user" type="com.pr.project.model.User" />
		<typeAlias alias="loginip" type="com.pr.project.model.LoginIp" />
		<typeAlias alias="message" type="com.pr.project.model.Message"/>
